<Window
    x:Class="AirConditionerShop.TruongMinhNhat.CreateWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:AirConditionerShop.TruongMinhNhat"
    mc:Ignorable="d"
    Title="DetailWindow"
    Height="450"
    Width="800">
    <Grid>
        <Label
            x:Name="DetailWindowMode"
            Content="Create new AirConditioner"
            FontSize="15"
            HorizontalAlignment="Left"
            Margin="44,25,0,0"
            VerticalAlignment="Top" />
        <Label
            Content="AirConditionalId"
            HorizontalAlignment="Left"
            Margin="45,96,0,0"
            VerticalAlignment="Top" />
        <Label
            Content="Name"
            HorizontalAlignment="Left"
            Margin="44,157,0,0"
            VerticalAlignment="Top" />
        <TextBox
            x:Name="AirconditionerName"
            HorizontalAlignment="Left"
            Height="23"
            Margin="44,122,0,0"
            TextWrapping="Wrap"
            VerticalAlignment="Top"
            Width="188" />
        <TextBox
            x:Name="AirconditionerIdTextBox"
            HorizontalAlignment="Left"
            Height="23"
            Margin="44,183,0,0"
            TextWrapping="Wrap"
            VerticalAlignment="Top"
            Width="188" />
        <Label
            Content="Warranty"
            HorizontalAlignment="Left"
            Margin="45,221,0,0"
            VerticalAlignment="Top" />
        <TextBox
            x:Name="Password_Copy"
            HorizontalAlignment="Left"
            Height="23"
            Margin="45,247,0,0"
            TextWrapping="Wrap"
            VerticalAlignment="Top"
            Width="188" />
        <Label
            Content="Sound Pressure Level"
            HorizontalAlignment="Left"
            Margin="45,286,0,0"
            VerticalAlignment="Top" />
        <TextBox
            x:Name="Password_Copy1"
            HorizontalAlignment="Left"
            Height="23"
            Margin="45,312,0,0"
            TextWrapping="Wrap"
            VerticalAlignment="Top"
            Width="188" />
        <Label
            Content="Feature Function"
            HorizontalAlignment="Left"
            Margin="45,352,0,0"
            VerticalAlignment="Top" />
        <TextBox
            x:Name="Password_Copy2"
            HorizontalAlignment="Left"
            Height="23"
            Margin="45,378,0,0"
            TextWrapping="Wrap"
            VerticalAlignment="Top"
            Width="188" />
        <Label
            Content="Quantity"
            HorizontalAlignment="Left"
            Margin="256,96,0,0"
            VerticalAlignment="Top" />
        <TextBox
            x:Name="Email_Copy"
            HorizontalAlignment="Left"
            Height="23"
            Margin="255,122,0,0"
            TextWrapping="Wrap"
            VerticalAlignment="Top"
            Width="188" />
        <Label
            Content="Dollar Price"
            HorizontalAlignment="Left"
            Margin="257,157,0,0"
            VerticalAlignment="Top" />
        <TextBox
            x:Name="Email_Copy1"
            HorizontalAlignment="Left"
            Height="23"
            Margin="256,183,0,0"
            TextWrapping="Wrap"
            VerticalAlignment="Top"
            Width="188" />
        <Label
            Content="Supplier"
            HorizontalAlignment="Left"
            Margin="258,221,0,0"
            VerticalAlignment="Top" />
        <ComboBox
            x:Name="SupplierComboBox"
            HorizontalAlignment="Left"
            Height="23"
            Margin="258,247,0,0"
            VerticalAlignment="Top"
            Width="185" />

    </Grid>
</Window>

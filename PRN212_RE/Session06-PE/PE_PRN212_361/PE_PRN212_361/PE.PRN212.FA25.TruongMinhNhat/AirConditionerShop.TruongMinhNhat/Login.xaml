<Window
    x:Class="AirConditionerShop.TruongMinhNhat.Login"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:AirConditionerShop.TruongMinhNhat"
    mc:Ignorable="d"
    Title="MainWindow"
    Height="330"
    Width="600"
    WindowStartupLocation="CenterScreen">
    <Grid>
        <Label
            Content="Email"
            HorizontalAlignment="Left"
            Margin="207,75,0,0"
            VerticalAlignment="Top" />
        <Label
            Content="Password"
            HorizontalAlignment="Left"
            Margin="206,136,0,0"
            VerticalAlignment="Top" />
        <TextBox
            x:Name="Email"
            HorizontalAlignment="Center"
            Height="23"
            Margin="0,101,0,0"
            TextWrapping="Wrap"
            VerticalAlignment="Top"
            Width="188" />
        <TextBox
            x:Name="Password"
            HorizontalAlignment="Center"
            Height="23"
            Margin="0,162,0,0"
            TextWrapping="Wrap"
            VerticalAlignment="Top"
            Width="188" />
        <Button
            Content="Login"
            HorizontalAlignment="Center"
            Margin="0,207,0,0"
            VerticalAlignment="Top"
            Width="188"
            Click="Button_Click" />

    </Grid>
</Window>
